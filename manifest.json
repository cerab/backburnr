{
  "name": "Backburnr",
  "description": "An easy way to catalogue and randomly seclet bookmarks!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "bookmarks"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/backburnr.png",
      "32": "/images/backburnr.png",
      "48": "/images/backburnr.png",
      "128": "/images/backburnr.png"
    }
  },
  "icons": {
    "16": "/images/backburnr.png",
    "32": "/images/backburnr.png",
    "48": "/images/backburnr.png",
    "128": "/images/backburnr.png"
  }
}